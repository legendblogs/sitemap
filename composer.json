{
    "name": "legendblogs/sitemap",
    "description": "Create and generate sitemaps with ease",
    "keywords": [
        "legendblogs",
        "sitemap"
    ],
    "homepage": "https://github.com/legendblogs/sitemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Amit Pandey",
            "email": "info@legendblogs.com",
            "homepage": "https://legendblogs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.3",
        "illuminate/support": "^11.10",
        "guzzlehttp/guzzle": "^7.8",
        "nesbot/carbon": "^3.6",
        "spatie/crawler": "^8.2",
        "symfony/dom-crawler": "^7.1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/phpunit-snapshot-assertions": "^4.0",
        "spatie/temporary-directory": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "legendblogs\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "legendblogs\\Sitemap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "legendblogs\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}